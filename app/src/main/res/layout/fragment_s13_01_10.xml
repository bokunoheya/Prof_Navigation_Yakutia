<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.pc.profnavykt.ProfessiiCode.S13_01_10">

    <TextView
        android:id="@+id/upText"
        style="@style/TextApp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/s13_01_10" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/spec_t"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView18"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="16dp"
                    android:src="@drawable/round_small_orange"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="64dp"
                    android:text="@string/kv"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/imageView18"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView26"
                style="@style/spec_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:text="@string/kv_text_s13_01_10" />

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/spec_t"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="16dp"
                    android:src="@drawable/round_small_blue"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="64dp"
                    android:text="@string/lvl"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView28"
                style="@style/spec_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginBottom="20dp"
                android:text="@string/minus" />

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:shrinkColumns="0"
                style="@style/spec_t"
                >

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_weight="2"
                        style="@style/TextViewStandard">

                        <ImageView
                            android:id="@+id/imageView8"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginStart="10dp"
                            android:src="@drawable/round_small_orange"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.486" />

                        <TextView
                            android:id="@+id/textView31"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:gravity="center|start"
                            android:text="@string/mesta"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.225"
                            app:layout_constraintStart_toEndOf="@+id/imageView8"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_weight="1"
                        style="@style/TextViewStandard2">

                        <ImageView
                            android:id="@+id/imageView9"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:src="@drawable/round_small_yellow"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.486" />

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="74dp"
                            android:layout_height="0dp"
                            android:gravity="center|start"
                            android:text="@string/kol_vo"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.157"
                            app:layout_constraintStart_toEndOf="@+id/imageView9"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_weight="1"
                        style="@style/TextViewStandard2">

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:src="@drawable/round_small_green"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.486" />

                        <TextView
                            android:id="@+id/textView29"
                            android:layout_width="68dp"
                            android:layout_height="0dp"
                            android:gravity="center|start"
                            android:text="@string/srok"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toEndOf="@+id/imageView3"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.583" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </TableRow>
            </TableLayout>


            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:shrinkColumns="0"
                >

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/o9"
                        style="@style/TextViewStandard"
                        android:layout_weight="2"
                        android:background="@drawable/border"
                        android:text="@string/ooo9" />

                    <TextView
                        android:id="@+id/mesto_o9"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                    <TextView
                        android:id="@+id/srok_o9"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                </TableRow>

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/o11"
                        android:layout_weight="2"
                        style="@style/TextViewStandard"
                        android:background="@drawable/border"
                        android:text="@string/soo11"
                        />

                    <TextView
                        android:id="@+id/mesto_o11"
                        android:layout_weight="1"
                        style="@style/TextViewStandard"
                        android:background="@drawable/border"
                        />
                    <TextView
                        android:id="@+id/srok_o11"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                </TableRow>


                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/zo9"
                        style="@style/TextViewStandard"
                        android:layout_weight="2"
                        android:background="@drawable/border"
                        android:text="@string/zoo9" />

                    <TextView
                        android:id="@+id/mesto_z9"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                    <TextView
                        android:id="@+id/srok_z9"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                </TableRow>

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/z11"
                        android:layout_weight="2"
                        style="@style/TextViewStandard"
                        android:background="@drawable/border"
                        android:text="@string/zsoo11"
                        />

                    <TextView
                        android:id="@+id/mesto_z11"
                        android:layout_weight="1"
                        style="@style/TextViewStandard"
                        android:background="@drawable/border"
                        />
                    <TextView
                        android:id="@+id/srok_z11"
                        style="@style/TextViewStandard"
                        android:layout_weight="1"
                        android:background="@drawable/border"
                        />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
