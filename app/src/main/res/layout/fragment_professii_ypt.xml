<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:theme="@style/buttonStyle2"
    tools:context="com.pc.profnavykt.CollegesProfessii.ProfessiiYpt" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/button1"
                    style="@style/buttonStyle4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heightList"
                    android:background="@drawable/block_i8"
                    android:paddingLeft="100dp"
                    android:text="@string/s08_01_14"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
               >

                <Button
                    android:id="@+id/button2"
                    style="@style/buttonStyle4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heightList"
                    android:background="@drawable/block_i8"
                    android:paddingLeft="100dp"
                    android:text="@string/s08_01_18"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <Button
                    android:id="@+id/button3"
                    style="@style/buttonStyle4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heightList"
                    android:background="@drawable/block_i13"
                    android:paddingLeft="100dp"
                    android:text="@string/s13_01_03"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <Button
                    android:id="@+id/button4"
                    style="@style/buttonStyle4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heightList"
                    android:background="@drawable/block_i15"
                    android:paddingLeft="100dp"
                    android:paddingRight="10dp"
                    android:text="@string/s15_01_05"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="4dp">

                <Button
                    android:id="@+id/button5"
                    style="@style/buttonStyle4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heightList"
                    android:background="@drawable/block_i15"
                    android:paddingLeft="100dp"
                    android:paddingRight="10dp"
                    android:text="@string/s15_01_26"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/button6"
                style="@style/buttonStyle4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/heightList"
                android:layout_marginBottom="4dp"
                android:background="@drawable/block_i29"
                android:paddingLeft="100dp"
                android:paddingRight="10dp"
                android:text="@string/s29_01_28" />

            <Button
                android:id="@+id/button7"
                style="@style/buttonStyle4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/heightList"
                android:layout_marginBottom="4dp"
                android:background="@drawable/block_i43"
                android:paddingLeft="100dp"
                android:paddingRight="10dp"
                android:text="@string/s43_01_07" />

            <Button
                android:id="@+id/button8"
                style="@style/buttonStyle4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/heightList"
                android:layout_marginBottom="4dp"
                android:background="@drawable/block_i35"
                android:paddingLeft="100dp"
                android:paddingRight="10dp"
                android:text="@string/s35_01_14" />

            <Button
                android:id="@+id/button9"
                style="@style/buttonStyle4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/heightList"
                android:layout_marginBottom="4dp"
                android:background="@drawable/block_i54"
                android:paddingLeft="100dp"
                android:paddingRight="10dp"
                android:text="@string/s54_01_02" />


        </LinearLayout>
    </ScrollView>

</LinearLayout>